  title = {Fitting {{Ordinal Factor Analysis Models With Missing Data}}: {{A Comparison Between Pairwise Deletion}} and {{Multiple Imputation}}},
  shorttitle = {Fitting {{Ordinal Factor Analysis Models With Missing Data}}},
