@article{li2016sem,
  title = {The Performance of {{ML}}, {{DWLS}}, and {{ULS}} Estimation with Robust Corrections in Structural Equation Models with Ordinal Variables},
  author = {Li, Cheng Hsien},
  year = {2016},
  month = sep,
  journal = {Psychological Methods},
  volume = {21},
